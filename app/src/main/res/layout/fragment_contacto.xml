<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Contacto">
    <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:background="#5e5d55">
            <Button
                    android:text="Denuncias"
                    android:id="@+id/button" app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintTop_toBottomOf="@+id/textView3"
                    style="@style/estilo_boton" android:textColor="#FFFFFF" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:text="Claves"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/button" android:textColor="#FFFFFF"
                    style="@style/estilo_boton" android:fontFamily="@font/raleway_bold" android:textSize="24sp"/>
            <Button
                    android:text="Saldos a Pagar"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button3" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button2"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:text="Saldos Disponibles"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button3"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:text="club La Nación"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button5" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button4"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:text="Galicia Seguros"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button6" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button5"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:text="HBO Go!"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button7" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button6"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:fontFamily="@font/raleway_bold"
                    android:textSize="24sp"/>
            <Button
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/button8" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/button7"
                    android:textColor="#FFFFFF" style="@style/estilo_boton" android:text="Aviso Viaje al Exterior"
                    android:fontFamily="@font/raleway_bold" android:textSize="24sp"/>
            <TextView
                    android:text="Atención a Clientes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="24dp"
                    android:fontFamily="@font/raleway_bold" android:textSize="36sp" android:textColor="#ECECEC"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>
</FrameLayout>